{
	"ConnectionStrings": {
		"Default": "Server=server_name;Database=database_name;Trusted_Connection=True;MultipleActiveResultSets=true"
	},
	"Logging": {
		"LogLevel": {
			"Default": "Information",
			"Microsoft": "Warning",
			"Microsoft.Hosting.Lifetime": "Information"
		}
	},
	"Data": {
		"MetadataCache": true, /* default: false */
		"ConnectionTimeout": "00:00:10" /* default: "00:00:30" */
	},
	"Application": {
		"Path": "path_to_application",
		"UserMenu": "user_menu_storage_procedure",
		"Theme": "theme.color",
		"Environment": {
			"Watch": true, /* default: false*/
			"Stage": "debug" /* default: "debug" */
		},
		"MultiTenant": false,
		"MultiCompany": false
	}
}

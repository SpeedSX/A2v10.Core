{
	"ConnectionStrings": {
		"Default": "Server=localhost;Database=database_name;Trusted_Connection=True;MultipleActiveResultSets=true"
	},
	"Logging": {
		"LogLevel": {
			"Default": "Information",
			"Microsoft": "Warning",
			"Microsoft.Hosting.Lifetime": "Information"
		}
	},
	"AllowedHosts": "*",
	"Data": {
		"MetadataCache": false,
		"CommandTimeout": "00:00:30"
	},
	"Application": {
		"Environment": {
			"Watch": true,
			"Stage": "debug"
		},
		"MultiTenant": false,
		"MultiCompany": false
	},
	"MailSettings": {
		"Host": "smtp.gmail.com",
		"Port": 587,
		"From": "qela@app.mail",
		"UserName": "string",
		"Password": "password",
		"EnableSsl": true,
		"DeliveryMethod": "FileSystem",
		"PickupDirectoryLocation": "c:\temp",
		"SkipCertificateValidation": false
	}
}

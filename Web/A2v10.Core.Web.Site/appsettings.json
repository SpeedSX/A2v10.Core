{
	"ConnectionStrings": {
		"Default": "Server=localhost;Database=database_name;Trusted_Connection=True;MultipleActiveResultSets=true"
	},
	"Logging": {
		"LogLevel": {
			"Default": "Information",
			"Microsoft": "Warning",
			"Microsoft.Hosting.Lifetime": "Information"
		}
	},
	"AllowedHosts": "*",
	"Data": {
		"MetadataCache": true,
		"CommandTimeout": "00:00:30"
	},
	"Application": {
		"Path2": "c:/git/a2v10/apps/develop",
		"UserMenu": "a2ui.[Menu.Simple.User.Load]",
		"Theme": "advance.blue",
		"Modules": {
			"Main": {
				"Default": true,
				"Path": "main_module_path"
			},
			"Personnel": {
				"Path": "personnle_module_path"
			}
		},
		"Environment": {
			"Watch": true,
			"Stage": "debug"
		},
		"MultiTenant": false,
		"MultiCompany": false
	}

}
